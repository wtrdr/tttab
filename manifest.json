{
  "name": "tttab",
  "description" : "Remember And Restore Your Favorite WebPage.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cmd.js", "watch.js"]
    }
  ],
  "background": {
    "scripts": ["helper.js", "background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "list.html"
  },
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+T",
        "mac": "MacCtrl+Shift+T"
      }
    }
  }
}
