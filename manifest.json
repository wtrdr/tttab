{
  "name": "tttab",
  "description" : "Remember And Restore Your Favorite WebPage.",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cmd.js", "watch.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
